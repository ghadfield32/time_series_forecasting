    "from py2neo import Graph\n",
    "\n",
    "def connect_neo4j():\n",
    "    graph = Graph(\"bolt://neo4j_username:password@neo4j_host:7687\")\n",
    "    \n",
    "    # Check connection\n",
    "    try:\n",
    "        print(\"Connected to Neo4j:\", graph)\n",
    "    except Exception as e:\n",
    "        print(f\"Connection failed: {e}\")\n",
    "\n",
    "connect_neo4j()\n"
   ]
  }
